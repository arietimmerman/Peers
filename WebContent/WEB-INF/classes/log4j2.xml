<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" name="RoutingTest" packages="org.apache.logging.log4j.test">

	<Appenders>
		<Console name="STDOUT">
			<PatternLayout pattern="%m%n" />
		</Console>

	</Appenders>

	<Loggers>
		<Root level="trace">
			<AppenderRef ref="STDOUT" />
		</Root>
	</Loggers>

</Configuration>